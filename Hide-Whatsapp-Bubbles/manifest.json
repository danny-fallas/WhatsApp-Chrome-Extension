{
    "manifest_version": 2,
    "name": "Hide Whatsapp Bubbles",
    "version": "1.9.10",
    "description": "Don't be THAT guy.",
    "icons": {
        "32": "32.png",
        "128": "128.png"
    },
    "browser_action": {
        "default_icon": "32.png",
        "default_title": "Turn ON"
    },
    "content_scripts": [
        {
            "js": [
                "contentscript.js"
            ],
            "matches": [
                "*://*.whatsapp.com/*"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "web_accessible_resources": [
        "background.js",
        "troll.png"
    ],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}